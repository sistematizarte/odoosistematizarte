<?xml version='1.0' encoding='utf-8'?>
<odoo>
<!-- FORMULARIO DE VISTA TREE -->
    <record id="account14_form_views" model="ir.ui.view">
        <field name="name">account14.form.view</field>
        <field name="model">datos</field>
        <field name="arch" type='xml'>
            <form string="datos">
                <sheet>
                    <group>
                        <group>
                            <field name="account"/>
                            <field name="project" />
                            <field name="prueba" />
                        </group>
                        <group>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!-- VISTA TREE -->
    <record id="datos_tree" model="ir.ui.view">
        <field name="name">datos.tree</field>
        <field name="model">datos</field>
        <field name="arch" type="xml">
            <tree string="Datos">
                <field name="account"/>
                <field name="project" />
                <field name="prueba" />
            </tree>
        </field>
    </record>
</odoo>